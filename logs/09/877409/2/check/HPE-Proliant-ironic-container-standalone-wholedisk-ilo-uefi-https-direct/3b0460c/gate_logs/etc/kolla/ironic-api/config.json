{
    "command": "/usr/sbin/httpd -DFOREGROUND",
    "config_files": [
        {
            "source": "/var/lib/kolla/config_files/ironic.conf",
            "dest": "/etc/ironic/ironic.conf",
            "owner": "ironic",
            "perm": "0600"
        },
        {
            "source": "/var/lib/kolla/config_files/ironic-api-wsgi.conf",
            "dest": "/etc/httpd/conf.d/ironic-api-wsgi.conf",
            "owner": "ironic",
            "perm": "0600"
        }    ],
    "permissions": [
        {
            "path": "/var/log/kolla/ironic",
            "owner": "ironic:ironic",
            "recurse": true
        }
    ]
}
